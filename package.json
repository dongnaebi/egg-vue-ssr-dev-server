    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "autoprefixer": "^9.6.1",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.7.0",
    "eslint-config-enough": "^0.2.8",
    "eslint-config-standard": "^12.0.0",
